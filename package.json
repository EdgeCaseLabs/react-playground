    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.3.13",
    "babelify": "^7.2.0",
    "body-parser": "^1.15.0",
    "bootstrap": "^4.0.0-alpha.3",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "express": "^4.13.4",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "jest": "^14.1.0",
