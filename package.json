{
  "name": "react-playground",
  "description": "Boiler Plate React App",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/edgecaselabs/react-playground"
  },
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
